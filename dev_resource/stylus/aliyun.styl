.header
	width 100%
	height 90px
	position absolute
	z-index 99
	.top
		width 1224px
		height 90px
		margin 0 auto
		.logo
			float left
			width 236px
			height 44px
			line-height 90px
			margin-top 16px
			background-image url("../img/logo.png")
			background-repeat no-repeat
		.nav
			float left
			width 568px
			height 90px
			position relative
			.nav-list
				height 90px
				float left
				padding 0 18px
				line-height 90px
				font-size 16px
				color white
				cursor pointer
				transition all .4s .4s
				&:hover
					height 90px
					background-color #0096bb
				&:hover .hidden-content
					max-height 300
					transition-delay .4s
				.hidden-content
					background-color white
					position absolute
					width 928px
					left 0
					right 0
					max-height 0
					overflow hidden
					transition max-height .4s linear 0s
					box-shadow 0 2px 2px 0 rgba(0,0,0,.2)
					.hidden-container
						margin 25px 25px 10px
						.module-wrap1
							width 368px
							float left
							.module-item
								margin-bottom 20px
								.module-title
									color #333
									font-size 14px
									line-height 18px
									&:hover
										color #00A2CA
								.module-desc
									color #8E8E8E
									font-size 12px
									line-height 16px
						.module-wrap2
							width 171px
							float left
							margin-right 48px
							.module-item
								font-size 14px
								width 171px
								.module-title
									display block
									width 171px
									line-height 44px
									color #999
									border-bottom 1px solid #F2F2F2
								.module-desc
									padding-top 10px
									padding-bottom 20px
									a
										font-size 14px
										line-height 18px
										color #333
										&:hover
											color #00A2CA
						.module-wrap3
							width 368px			
							.module-item
								margin-bottom 20px
								.module-title
									color #333
									font-size 14px
									line-height 18px
									&:hover
										color #00A2CA
								.module-desc
									color #8E8E8E
									font-size 12px
									line-height 16px
							
		.login
			float right
			height 90px
			.item
				font-size 14px
				float left
				padding 0 20.5px
				color white
				line-height 90px
				position relative
				height 90px
				&:hover
					cursor pointer
				&:hover .right
					display block
				.icon
					margin-top 40px
					width 16px
					height 6px
					display inline-block
					border-bottom 1px solid
					vertical-align super
					&:before
						content ""
						display block
						border-top 1px solid
						width 16px
						height 0px
					&:after
						content ""
						display block
						border-bottom 1px solid
						width 16px
						height 10px
					&:hover
						transition height .3s
						height 10px
						margin-top 34px
						&:before
							height 10px
						&:after
							height 10px
				.right
					display none
					background-color white
					box-shadow 0 2px 2px rgba(0,0,0,.2)
					position absolute
					top 90px
					right 14px
					width 140px
					height auto
					padding-top 18px
					padding-left 20px
					a
						display block
						line-height 36px
						color #333
						font-size 12px
						&:hover
							color #00A2CA
.video
	position fixed
	right 0
	bottom 0
	min-width 100%
	min-height 100%
	z-index -5
.pages
	width 100%
	.main1
		height 500px
		position relative
		overflow hidden
		z-index 2
		.imgs
			width 100%
			height 500px
			position absolute
			li
				width 100%
				height 500px
				display none
				img
					position absolute
					top 0
					left 50%
					transform translateX(-50%)
		&:before
			position absolute
			right 50%
			border-right 1440px solid transparent
			display block
			content ""
			width 0
			height 0
			bottom 0
			z-index 98
			border-bottom 110px solid #fff
		&:after
			position absolute
			left 50%
			border-left 1440px solid transparent
			display block
			content ""
			width 0
			height 0
			bottom 0
			z-index 98
			border-bottom 110px solid #fff
		.main1-text
			height 500px
			overflow hidden
			position relative
			z-index 2
			.slide-item
				margin-left 20px
				display none
				position relative
				top -60px
				.center
					width 1224px
					margin 0 auto
					display block
					color white
					padding-bottom 80px
					
					
					h1
						font-size 48px
						font-weight 100
						text-shadow 0 0 3px rgba(0,0,0,.13)
						margin-top 160px
						margin-bottom 15px
					h2
						font-size 20px
						font-weight 100
						line-height 28px
						text-shadow 0 0 3px rgba(0,0,0,.13)
						padding-top 20px
						margin-bottom 37px
					.btn
						width 180px
						height 42px
						font-size 16px
						color white
						text-align center
						line-height 42px
						background-color transparent
						margin-top 20px
						display inline-block
						border 1px solid white
						transition all .3s cubic-bezier(.645,.045,.355,1)
						&:hover
							background-color rgba(255,255,255,.2)
			.slide-item:last-child
				.center
					width 1224px
					margin 0 auto
					text-align center
					.btn
						width 180px
						height 42px
						font-size 16px
						color white
						background-color #00A2CA
						border-color #00A2CA
						&:hover
							background-color #33b5d5
							border-color #33b5d5
